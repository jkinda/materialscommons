{
  "extends": "eslint:recommended",
  "plugins": [
    "angular"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "angular": true,
    "$": true,
    "module": true,
    "inject": true,
    "_add_json_callback": true,
    "_": true,
    "TreeModel": true,
    "isImage": true,
    "bytesToSizeStr": true,
    "Flow": true,
    "removeById": true,
    "differenceById": true,
    "localforage": true,
    "P": true,
    "require": true,
    "introJs": true,
    "ActionheroWebsocketClient": true
  },
  "rules": {
    "angular/log": 0,
    "no-console": 0,
    "angular/document-service": 0,
    "angular/angularelement": 0,
    "angular/window-service": 0,
    "angular/module-getter": 0,
    "angular/di": 0
  }
}

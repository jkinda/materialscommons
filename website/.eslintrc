{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
    "_add_json_callback": true,
    "_": true,
    "TreeModel": true,
    "isImage": true,
    "bytesToSizeStr": true,
    "Flow": true
  }, 
  "rules": {
    "angular/log": 0,
    "no-console": 0,
    "angular/document-service": 0
  }
}
